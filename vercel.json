{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/silviobaratto/browser",
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x",
      "includeFiles": "baml_client/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/chatbot/stream",
      "destination": "/api/serverless-chatbot-stream"
    },
    {
      "source": "/api/chatbot",
      "destination": "/api/serverless-chatbot"
    }
  ]
}
